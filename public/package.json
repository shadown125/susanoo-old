{
  "name": "susanoo",
  "version": "1.0.0",
  "description": "Susanoo CMS",
  "main": "index.js",
  "scripts": {
    "prebuild": "npm-run-all --parallel",
    "build": "npm-run-all --parallel build:*",
    "build:scripts": "webpack",
    "build:styles": "npm-run-all sass:build postcss:build",
    "build:images": "imagemin src/pics/*.{jpg,png} --out-dir=build",
    "watch": "npm-run-all --parallel watch:*",
    "watch:scripts": "webpack --watch",
    "watch:styles": "npm-run-all --parallel sass:watch postcss:watch",
    "sass:build": "sass src/styles:build --no-source-map",
    "sass:watch": "sass src/styles:build --no-source-map --watch",
    "postcss:build": "postcss build/*.css --replace --env production",
    "postcss:watch": "postcss build/*.css --watch --dir build --poll=1000",
    "pre-production": "grunt pre-production",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "UNLICENSED",
  "private": true,
  "browserslist": [
    "> 0.5%"
  ],
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-private-methods": "^7.13.0",
    "@babel/preset-env": "^7.13.9",
    "@testing-library/dom": "^7.29.6",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/user-event": "^12.8.1",
    "autoprefixer": "^10.2.4",
    "babel-loader": "^8.2.2",
    "copyfiles": "^2.4.1",
    "expose-loader": "^2.0.0",
    "imagemin": "^7.0.1",
    "imagemin-cli": "^6.0.0",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.7",
    "postcss-cli": "^8.3.1",
    "postcss-csso": "^5.0.0",
    "sass": "^1.32.8",
    "stylelint": "^13.11.0",
    "svgo": "^2.2.0",
    "terser-webpack-plugin": "^5.1.1",
    "webpack": "^5.24.3",
    "webpack-bundle-analyzer": "^4.3.0",
    "webpack-cli": "^4.5.0"
  },
  "dependencies": {
    "core-js": "^3.11.0",
    "countup.js": "^2.0.7",
    "foundation-sites": "^6.6.3",
    "jquery": "^3.6.0",
    "regenerator-runtime": "^0.13.7",
    "swiper": "^6.5.0",
    "what-input": "^5.2.10"
  }
}
